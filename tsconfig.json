{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "lib": ["ES2022", "DOM"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "@/ingest/*": ["./ingest/*"],
      "@/features/*": ["./features/*"],
      "@/personas/*": ["./personas/*"],
      "@/recommend/*": ["./recommend/*"],
      "@/guardrails/*": ["./guardrails/*"],
      "@/ui/*": ["./ui/*"],
      "@/eval/*": ["./eval/*"],
      "@/types/*": ["./types/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules", "dist"]
}
